{
  "name": "fairly-fast-formants",
  "description": "A library for formant extraction from audio.",
  "version": "0.1.0",
  "main": "index.html",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.37",
    "svelte-canvas": "^0.7.1",
    "svelte-preprocess": "^4.10.3",
    "svelte-routing": "^1.6.0",
    "typescript": "^4.5.5",
    "vite": "^2.6.14",
    "vite-plugin-wasm-pack": "^0.1.9"
  },
  "scripts": {
    "dev": "vite --config vite.site.config.js",
    "build-lib": "vite build --config vite.build.config.js",
    "build-site": "vite build --config vite.site.config.js",
    "build-wasm": "wasm-pack build ./formants-wasm --target web",
    "clean-wasm": "rm -rf ./formants-wasm/pkg",
    "clean-js": "rm -rf dist"
  },
  "peerDependencies": {
    "mathjs": "^10.1.1"
  },
  "peerDependenciesMeta": {
    "mathjs": {
      "optional": true
    }
  }
}
